{
    "version": 2,
    "builds": [{
        "src": "/src/index.js",
        "use": "@vercel/node"
    }],
    "headers": [{
        "source": "/api/(.*)",
        "headers": [
            { "key": "Access-Control-Allow-Credentials", "value": "true" },
            { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/src/index.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE"
        ]
    }]
}